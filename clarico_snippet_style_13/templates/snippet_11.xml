<?xml version="1.0" encoding="utf-8"?>
<odoo>
		<template id="clarico_style_11" inherit_id="clarico_snippets.clarico_pricing_snippets" name="clarico Snippet" priority="24">
			<xpath expr="//div[@id='clarico_pricing_structure']/div[hasclass('o_panel_body')]" position="inside">
				<t t-snippet="clarico_snippet_style_13.clarico_snippet_style_13_template" t-thumbnail="/clarico_snippet_style_13/static/src/img/snippet_13.jpg"  /> 
							</xpath>
		</template>
		<template id="clarico_snippet_style_13_template" name="3 Column">
		<div class="oe_structure" />
				<section>
		<div class="pricing_whole_div_calss">
			<div class="div_sub_main_pricing">
					<div class="div_heading_pricing">
						<h1 class="snippet_heading lato-light font-IrisBlue">PRICING</h1>
						<!-- <hr class="hr_class_common_pricing border-grey"/>
						<p class="para_pricing_content lato-light font-black">Create an attractive-looking pricing section for your products.</p>
		 -->			</div>
					<div class="div_main_pricing_content">
						<div class="div_sub_main_pricing_content">
